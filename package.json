{
  "name": "directive-engine-v0.1-impl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "gen": "tsx src/cli/index.ts --nominal data/toy_nominal_poses.json --asbuilt data/toy_asbuilt_poses.json --constraints data/toy_constraints.json --out out/directives.json",
    "dev": "vite",
    "copy:demo-data": "cp datasets/toy_v0_1/toy_* demo/public/",
    "build": "vite build",
    "prebuild": "npm run copy:demo-data",
    "preview": "vite preview",
    "test": "vitest && npm run test:legacy",
    "test:legacy": "tsx src/test/renderHelpers.ts && tsx src/test/golden.ts"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.9"
  }
}
